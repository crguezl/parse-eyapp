# File TSPostfix3.eyp. To use it:
# eyapp TSPostfix3.eyp; usetspostfix3.pl
%right '=' 
%left '-' '+' 
%left '*' '/' 
%left NEG 
%metatree

%%

line:
      %name PROG
      exp <%name EXP + ';' >
;
exp:
      %name NUM
      NUM
    | %name VAR
      VAR
    | %name ASSIGN
      VAR '=' exp
    | %name PLUS
      exp '+' exp
    | %name MINUS
      exp '-' exp
    | %name TIMES
      exp '*' exp
    | %name DIV
      exp '/' exp
    | %name NEG
      '-' exp %prec NEG
    | '(' exp ')' 
;

%%
